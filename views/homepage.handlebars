<div class="wrapper-grid"></div>
{{#each posts as |post|}}
  <div class="card w-75">
    <div class="card-body">
      <h5 class="card-title">{{ post.post_title }}</h5>
      <p class="card-text">
        {{post.blog_content}}
      </p>
      <a href="#" class="btn btn-primary">Button</a>
    </div>
  </div>
{{/each}}
</div>